<p id="notice"><%= notice %></p>

<h1>Persosns</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @persosns.each do |persosn| %>
      <tr>
        <td><%= link_to 'Show', persosn %></td>
        <td><%= link_to 'Edit', edit_persosn_path(persosn) %></td>
        <td><%= link_to 'Destroy', persosn, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Persosn', new_persosn_path %>
